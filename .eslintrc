{
    "parser": "@babel/eslint-parser",
    "env": {
		"browser": false,
		"node": true,
        "es6": true,
        "jest/globals": true
	},
    "plugins": [
        "jest"
    ],
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "globals": {
    },
    "rules": {
        "no-var": "warn",
        "quotes": [
            "warn",
            "single"
        ]
    }
}